@model IEnumerable<SeguimientoReclamos>

@{
    ViewData["Title"] = "Index";
}

<h1>Seguimiento Reclamos</h1>

<p>
    <a asp-action="Create">Nuevo Seguimiento</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Reclamos.IdReclamo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reclamos.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaSeguimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NotificacionCliente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Reclamos.IdReclamo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Reclamos.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaSeguimiento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NotificacionCliente)
                </td>
            </tr>
        }
    </tbody>
</table>
